        <ui:Button text="Remove Effect" display-tooltip-when-elided="true" name="RemoveEffectButton" />
    </ui:GroupBox>
    <ui:VisualElement name="VisualElement" style="flex-direction: row; flex-grow: 1; align-items: stretch; justify-content: space-around;">
        <ui:VisualElement style="flex-grow: 1; align-items: stretch; justify-content: space-around; flex-direction: column;">
            <ui:Label text="Status Effects" display-tooltip-when-elided="true" style="align-items: stretch; -unity-text-align: upper-center; margin-top: 0; -unity-font-style: bold; visibility: visible; margin-left: 0; margin-right: 0; margin-bottom: 0; justify-content: flex-start; flex-grow: 0;" />
            <ui:ListView focusable="true" name="EffectList" style="align-items: auto; flex-direction: column;" />
        </ui:VisualElement>
    </ui:VisualElement>
